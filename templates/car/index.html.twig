{% extends 'base.html.twig' %}

{% block title %}Car{% endblock %}

{% block body %}

{% for theCar in car %}
    <div class="col-lg-4 all web">
        <div class="card">
            <div class="card-head">
            <a href="{{ path('car_show', {id: theCar.id})}}">
                <div class="header">
                    <h2>{{theCar.marque}}</h2>
                    <p>{{theCar.modele}}</p>
                </div>
            </a>
            </div>
        </div>
    </div>
{% endfor %}
{% endblock %}
